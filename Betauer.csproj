<Project Sdk="Godot.NET.Sdk/3.3.0">
    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <EnableDefaultCompileItems>False</EnableDefaultCompileItems>
        <LangVersion>8.0</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove=".mono/**" />
        <Compile Remove="DemoGame\**" />
        <Compile Remove="Betauer\**" />
    </ItemGroup>
    <ItemGroup>
        <Compile Remove="Betauer.Generator\**" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="Betauer\Betauer.Animation\Betauer.Animation.csproj" />
        <ProjectReference Include="Betauer\Betauer.Core\Betauer.Core.csproj" />
        <ProjectReference Include="Betauer\Betauer.DI\Betauer.DI.csproj" />
        <ProjectReference Include="Betauer\Betauer.GameTools\Betauer.GameTools.csproj" />
        <ProjectReference Include="Betauer\Betauer.GodotAction\Betauer.GodotAction.csproj" />
        <ProjectReference Include="Betauer\Betauer.StateMachine\Betauer.StateMachine.csproj" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Remove=".mono/**" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="NUnit" Version="3.13.2" />
    </ItemGroup>
    <ItemGroup>
        <Content Include="*" Exclude="mono_crash*" />
    </ItemGroup>
</Project>
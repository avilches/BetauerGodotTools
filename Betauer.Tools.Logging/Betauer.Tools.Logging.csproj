<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <IsPackable>false</IsPackable>
        <LangVersion>preview</LangVersion>
        <RootNamespace>Betauer.Tools.Logging</RootNamespace>
        <PackageId>Betauer.Tools.Logging</PackageId>
        <AssemblyName>Betauer.Tools.Logging</AssemblyName>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <Reference Include="GodotSharp, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
            <HintPath>$(SolutionDir)/.mono/assemblies/Debug/GodotSharp.dll</HintPath>
        </Reference>
        <PackageReference Include="ZString" Version="2.5.0" />
    </ItemGroup>
</Project>
